{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "bootloadertype": "ipxe",
                "kernel": "https://validation.deepin.io/tftpboot/vmlinuz",
                "nfsrootfs": "https://ci.deepin.io/job/rr-builder-rootfs/147/artifact/rootfs.tar.gz",
                "ramdisk": "https://validation.deepin.io/tftpboot/initrd.img",
                "target_type": "deepin"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "https://github.com/qiujieqiong/testlink-robotframework-integration.git",
                        "testdef": "lava-test.yaml"
                    }
                ],
                "timeout": 900
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "https://validation.deepin.io/RPC2/",
                "stream": "/anonymous/test/"
            }
        }
    ],
    "target": "102",
    "job_name": "lava-test",
    "timeout": 18000
}